<ctl:MetroWindow x:Class="SixCloud.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SixCloud"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:ctl="clr-namespace:SixCloudCustomControlLibrary.Controls;assembly=SixCloudCustomControlLibrary"
        mc:Ignorable="d"
        MinHeight="450" Height="450" Width="800" MetroWindowLayout="WideAngle">
    <StackPanel>
        <Button Content="HelloWorld" Click="Button_Click" Style="{StaticResource MaterialDesignFlatAccentButton}" FocusVisualStyle="{x:Null}"/>
        <Button Content="Change Color" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Button_Click_1"/>
        <ctl:RenamableTextBox CurrentName="Hello World" IsRenamable="True"/>
    </StackPanel>
</ctl:MetroWindow>
